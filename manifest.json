{
  "name": "Editorialize",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Adds some emoji to the NY Times opinion section",
  "homepage_url": "https://muffinlabs.com/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://www.nytimes.com/*/opinion/*"
      ],
      "js": [
        "src/inject/inject.js"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ]
}
